"amixer set Master playback 5%+"
  XF86AudioRaiseVolume

"amixer set Master playback 5%-"
  XF86AudioLowerVolume

"amixer set Master toggle"
  XF86AudioMute

"xbacklight -dec 10"
  XF86MonBrightnessDown

"xbacklight -inc 10"
  XF86MonBrightnessUp

"kbdlight up"
  XF86KbdBrightnessUp

"kbdlight down"
  XF86KbdBrightnessDown

"mpc -q toggle"
  XF86AudioPlay

"mpc -q stop"
  XF86AudioStop

"mpc -q prev"
  XF86AudioPrev

"mpc -q next"
  XF86AudioNext
